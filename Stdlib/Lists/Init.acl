let lists = put Lists in
let empty = put Empty in
let cell = put Cell in
let reverse = put Reverse in
let naturals = put ../Naturals/Naturals in
let zero = put ../Naturals/Zero in
let succ = put ../Naturals/Succ in
let compose2 = put ../Functions/Compose2 in

let init_rev =
    'init_rev [
        @ zero %f -> empty;
        @ (succ %n) %f -> cell (%f %n) (@ %n %f)
    ]
    : +naturals ^ (+naturals ^ +%e) ^ +lists +%e
in

compose2 reverse init_rev

