let lists = put Lists in
let empty = put Empty in
let cell = put Cell in
let booleans = put ../Booleans/Booleans in
let if = put ../Booleans/If in

'filter [
    @ %f empty -> empty;
    @ %f (cell %x %lst) -> if (%f %x) (cell %x (@ %f %lst)) (@ %f %lst)
]
: (+%e ^ +booleans) ^ +lists +%e ^ +lists +%e

