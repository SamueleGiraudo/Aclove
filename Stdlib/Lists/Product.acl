let lists = put Lists in
let empty = put Empty in
let cell = put Cell in
let append = put Append in
let map = put Map in
let pairs = put ../Products/Pairs in
let pair = put ../Products/Pair in

'product [
    @ empty %lst -> empty;
    @ (cell %x %lst1) %lst2 -> append (@ %lst1 %lst2) (map (pair %x) %lst2)
]
: +lists +%e1 ^ +lists +%e2 ^ (+lists (+pairs +%e1 +%e2))

