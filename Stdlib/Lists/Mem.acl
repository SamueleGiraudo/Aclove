let lists = put Lists in
let empty = put Empty in
let cell = put Cell in
let booleans = put ../Booleans/Booleans in
let false = put ../Booleans/False in
let or = put ../Booleans/Or in

'mem [
    @ %f %x empty -> false;
    @ %f %x1 (cell %x2 %lst) -> or (%f %x1 %x2) (@ %f %x1 %lst)
]
: (+%e ^ +%e ^ +booleans) ^ +%e ^ +lists +%e ^ +booleans

